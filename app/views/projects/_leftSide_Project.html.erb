<div class="row project-row">
  <div class="col-md-4">
    <%= image_tag val.projImage.url(:medium), class: "img-responsive img-rounded project-img center-block" %>
  </div>
  <div class="col-md-8">
    <h3><%= link_to_unless(val.proj_link.nil?, val.full_title, val.proj_link, target: "_blank") %></h3>
    <p class="well"><%= val.description %></p>
  </div>
</div>
