<% provide(:title, "Projects") %>

<div class="row" style="margin-bottom: 8px;">
  <div class="container text-left">
    <%= link_to "Add New Project", project_new_path, class: 'btn btn-primary' %>
    <%= link_to "Forget Password Rest", forget_password_reset_path, method: :put, class: 'btn btn-warning' if @user.reset_digest != nil %>
  </div>
</div>


<ol class="projects">
  <%= render @projects %>
</ol>
